{
  "name": "pebble-dashboard",
  "version": "1.0.0",
  "description": "Investment tracking dashboard",
  "private": true,
  "main": "index.js",
  "scripts": {
    "electron:dev": "PEBBLE_ENV=development electron .",
    "electron:prod": "npm run build && PEBBLE_ENV=production electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saranshmanu/pebble-dashboard.git"
  },
  "keywords": [
    "pebble-dashboard"
  ],
  "author": "Saransh Mittal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saranshmanu/pebble-dashboard/issues"
  },
  "devDependencies": {
    "electron": "^24.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^5.0.1",
    "@ant-design/plots": "^1.2.5",
    "@babel/polyfill": "^7.12.1",
    "antd": "^5.4.0",
    "node-sass": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "rxdb": "^14.6.1",
    "rxjs": "^7.8.0",
    "uuid": "^9.0.0"
  }
}
